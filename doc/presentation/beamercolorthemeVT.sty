\ProvidesPackageRCS $Header: /cvsroot/latex-beamer/latex-beamer/themes/color/beamercolorthemewolverine.sty,v 1.6 2007/01/28 20:48:24 tantau Exp $

\mode<presentation>

\definecolor{darkblue}{rgb}{.54901960784313725490,.68627450980392156862,.67843137254901960784}
\definecolor{vtmaroon}{rgb}{.4,0,0}
\definecolor{vtorange}{rgb}{1,.4,0}

\setbeamercolor{alerted text}{fg=white!80!vtorange}
\setbeamercolor*{palette primary}{fg=white!60!black,bg=vtorange!85!vtmaroon}
\setbeamercolor*{palette secondary}{fg=white!70!black,bg=vtorange!60!vtmaroon}
\setbeamercolor*{palette tertiary}{bg=white!80!black,fg=vtorange!50!vtmaroon}
\setbeamercolor*{palette quaternary}{fg=white,bg=vtorange!0!vtmaroon}

\setbeamercolor*{sidebar}{fg=white,bg=vtmaroon}

\setbeamercolor*{palette sidebar primary}{fg=white!10!black}
\setbeamercolor*{palette sidebar secondary}{fg=white}
\setbeamercolor*{palette sidebar tertiary}{fg=white!50!black}
\setbeamercolor*{palette sidebar quaternary}{fg=vtorange!10!vtmaroon}

\setbeamercolor*{titlelike}{parent=palette primary}
\setbeamercolor{frametitle}{bg=vtorange!90!vtmaroon}
\setbeamercolor{frametitle right}{bg=vtorange!60!vtmaroon}

\setbeamercolor*{separation line}{}
\setbeamercolor*{fine separation line}{}

\mode
<all>
